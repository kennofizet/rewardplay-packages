{
  "name": "@company/rewardplay-frontend",
  "version": "1.0.0",
  "description": "RewardPlay frontend package - Token-based API client for RewardPlay",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js",
      "default": "./src/index.js"
    },
    "./*": "./src/*"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "keywords": [
    "vue",
    "vue3",
    "rewardplay",
    "game",
    "frontend",
    "component-library"
  ],
  "author": "KennoFizet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "vue": "^3.2.30",
    "axios": "^1.1.2"
  },
  "devDependencies": {
    "laravel-mix": "^6.0.49"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

